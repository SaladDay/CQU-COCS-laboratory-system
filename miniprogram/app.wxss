    height: 50rpx;
    line-height: 50rpx;
}

.fenger-top{
  width: 100%;
  height: 7px;
  background: #f0f0f0;
}

.fenger-top p{
  width: calc(100% - 14px);
  height: 7px;
  position: absolute;
  left: 7px;
  background: #fff;
  border-top: 1px dashed #f0f0f0;
}

.fenger-top p::before{
  content: '';
  position: absolute;
  width: 7px;
  height:7px;
  left: -7px;
  background-image: radial-gradient(circle 7px at 0 0, transparent 50px, #fff 50%);
}
.fenger-top p::after{
  content: '';
  position: absolute;
  width: 7px;
  height: 7px;
  right: -7px;
  background-image: radial-gradient(circle 7px at 100% 0, transparent 50px, #fff 50%);
}

.fenger-bottom{
  width: 100%;
  height: 7px;
  background: #f0f0f0;
}

.fenger-bottom p{
  width: calc(100% - 14px);
  height: 7px;
  position: absolute;
  left: 7px;
  background: #fff;
  border-bottom: 1px dashed #f0f0f0;
}
.fenger-bottom p::before{
  content: '';
  position: absolute;
  width: 7px;
  height: 7px;
  left: -7px;
  background-image: radial-gradient(circle 7px at 0 100%, transparent 50px, #fff 50%);
}
.fenger-bottom p::after{
  content: '';
  position: absolute;
  width: 7px;
  height: 7px;
  right: -7px;
  background-image: radial-gradient(circle 7px at 100% 100%, transparent 50px, #fff 50%);
}

