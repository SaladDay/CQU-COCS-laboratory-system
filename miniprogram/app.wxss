.wrap {
  width:100%;
  box-sizing:border-box;
  -moz-box-sizing:border-box; /* Firefox */
  -webkit-box-sizing:border-box; /* Safari */
  background-color: #f0f0f0;
  padding-bottom: 1rpx;
}

p {
  margin: 0;
  padding: 0;
}

.top-box {
  box-sizing:border-box;
  -moz-box-sizing:border-box; /* Firefox */
  -webkit-box-sizing:border-box; /* Safari */
  width: 100%;
  background: #fff;
  position: relative;
}
.middle-box {
  box-sizing:border-box;
  -moz-box-sizing:border-box; /* Firefox */
  -webkit-box-sizing:border-box; /* Safari */
  width: 100%;
  background: #fff;
  position: relative;
  border-radius: 4px 4px 0 0;
}


.bottom-box {
  box-sizing:border-box;
  -moz-box-sizing:border-box; /* Firefox */
  -webkit-box-sizing:border-box; /* Safari */
  width: 100%;
  background: #fff;
  position: relative;
  border-radius: 0 0 4px 4px;
}

